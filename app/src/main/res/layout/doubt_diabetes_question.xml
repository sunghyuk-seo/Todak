<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="147dp"
        android:layout_weight="1"
        android:background="#84FBCC"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/corona_top_text"
        android:layout_width="133dp"
        android:layout_height="47dp"
        android:layout_marginTop="44dp"
        android:fontFamily="@font/cookierun2"
        android:text="당뇨 진단"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="@+id/view3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ScrollView
        android:id="@+id/scrollView_Cold"
        android:layout_width="374dp"
        android:layout_height="353dp"
        android:layout_margin="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_16q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="심한 갈증을 느낍니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_16c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_1q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="소변을 자주 봅니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_1c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:checked="false"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_2q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="잠을 자는데 어려움이 있습니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_2c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_3q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="상처가 잘 아물지 않습니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_3c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_4q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="옆구리에서 통증을 느낍니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_4c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_5q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="배뇨시 통증을 느낍니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_5c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_6q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="발이 저리거나 피가 잘 통하지 않는 것 같습니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_6c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_7q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="피부가 건조하거나 탄력이 떨어진 것 같습니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_7c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_8q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="체중이 눈에 띄게 줄었습니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_8c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />

            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="10dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Q1_9q"
                    android:layout_width="305dp"
                    android:layout_height="32dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/cookierun2"
                    android:text="최근들어 식욕이 늘었습니까?"
                    android:textAlignment="center" />

                <CheckBox
                    android:id="@+id/Q1_9c"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:onClick="onCheckButtonListener"
                    android:text="" />


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_marginBottom="10dp"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/Q1_10q"
                        android:layout_width="305dp"
                        android:layout_height="32dp"
                        android:layout_weight="1"
                        android:fontFamily="@font/cookierun2"
                        android:text="시야가 흐립니까?"
                        android:textAlignment="center" />

                    <CheckBox
                        android:id="@+id/Q1_10c"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:onClick="onCheckButtonListener"
                        android:text="" />


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginBottom="10dp"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/Q1_11q"
                            android:layout_width="305dp"
                            android:layout_height="32dp"
                            android:layout_weight="1"
                            android:fontFamily="@font/cookierun2"
                            android:text="극심한 피로감을 느낍니까?"
                            android:textAlignment="center" />

                        <CheckBox
                            android:id="@+id/Q1_11c"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:onClick="onCheckButtonListener"
                            android:text="" />

                    </LinearLayout>

                </LinearLayout>

            </LinearLayout>


        </LinearLayout>


    </ScrollView>

    <Button
        android:id="@+id/Q1_resultbutton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/cookierun2"
        android:text="결과보기"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView_Cold"
        app:layout_constraintVertical_bias="0.81" />

</androidx.constraintlayout.widget.ConstraintLayout>